<!DOCTYPE html>
<html lang="ko">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <script
      src="https://kit.fontawesome.com/fcee7a3dbf.js"
      crossorigin="anonymous"
    ></script>
    <style>
      h2 {
        color: red;
      }
    </style>
  </head>
  <body>
    <h1><i class="fa-solid fa-address-card"></i> Contact Pages</h1>
    <h2><%= heading %></h2>
    <ul class="user-list">
      <% users.forEach(user => { %>
      <li><strong>Name: </strong> <%= user.name %></li>
      <li>
        <strong>Email: </strong>
        <a href="mailto:<%= user.email %>"><%= user.email %></a>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
